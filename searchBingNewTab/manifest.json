{
    "name" : "deshapriya",
    "version" : "1.0",
    "manifest_version" : 3,
    "action" : {
        "default_popup" : "popup.html"
    },
    
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "bookmarks",
        "unlimitedStorage"
      ],
      "host_permissions": [
        "<all_urls>"
      ]
}